// Hamburger_nav Styles
style(type='text/css').
	/*Hamburguer fix IOS9*/
	body {-webkit-animation: bugfix infinite 1s;}
	@-webkit-keyframes bugfix {
		from {
			padding: 0;
		}
		to {
			padding: 0;
		}
	}
	/*Hamburguer fix IOS9*/
	/*Hamburger_nav*/
	*[class].show {
		display: block !important;
		width: auto !important;
		overflow: visible !important;
		float: none !important;
		max-height: inherit !important;
		line-height: inherit !important;
		width: 278px;
	}

	*[class].navbar-checkbox{display:none;}
	*[class].hamburger, *[class].hamburger > img{display:block;}
	*[class].close, *[class].close > img{display:none;}
	*[class].menu {
		visibility: hidden;
		opacity: 0;
		height: auto !important;
		position: absolute !important;
		top: 59px !important;
		left: 0px !important;
		transition: opacity .25s ease-in-out;
		-moz-transition: opacity .25s ease-in-out;
		-webkit-transition: opacity .25s ease-in-out;
		border-top: none;
		background-color: #ffffff;
		box-shadow: 4px 4px 5px -4px #999999 !important;
		display: none;
	}

	*[class].navbar-checkbox:checked ~ *[class].hamburger{display:none!important;}
	*[class].navbar-checkbox:checked ~ *[class].close, *[class].navbar-checkbox:checked ~ *[class].close > img {
		display: block !important;
		width: auto !important;
		overflow: visible !important;
		float: none !important;
		max-height: inherit !important;
		line-height: inherit !important;
		position: relative !important;
		top: 0px !important;
		z-index: 11 !important; /*left: 0px !important;*/
	}

	*[class].navbar-checkbox:checked ~ *[class].menu {
		visibility: visible !important;
		opacity: 1;
		z-index: 10 !important;
		transition-delay: 0s;
		display: block;
	}
	*[class].hamburger, *[class].close{cursor:pointer!important;}
	/*Hamburger_nav*/
	/*Hamburger_nav Styles*/
	@media only screen and (max-width: 640px) {
			.contentInteractive{display:none!important;}
			*[class=contentInteractive]{display:none!important;}
			/*interactive styes*/
			.mobileCont:checked ~ table *[class=contentInteractive]{
				display: block !important;
			}
			.mobileCont:checked ~ table *[class=nocontentInteractive] {
				display: none !important;
			}
		}
