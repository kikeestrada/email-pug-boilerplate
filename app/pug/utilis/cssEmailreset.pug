style(type='text/css').
	body {
		width: 100% !important;
		-webkit-text-size-adjust: 100%;
		-ms-text-size-adjust: 100%;
		margin: 0 !important;
		padding: 0 !important;
	}
	.ReadMsgBody {width: 100%;}
	.ExternalClass p,
	.ExternalClass span,
	.ExternalClass font,
	.ExternalClass td
	{line-height: 100%}
	.gmailfix {
		display:none;
		display:none!important;
	}
	table {
		border-collapse:collapse;
		mso-table-lspace:0;
		mso-table-rspace:0;
		padding:0;
	}
	table td {
		border-collapse: collapse !important;
		border-spacing: 0;
		border: 0 none;
		mso-table-lspace: 0pt;
		mso-table-rspace: 0pt;
	}
	p{margin:0 auto; text-align:center;}
	*{
		-webkit-text-size-adjust:none;
		-ms-text-size-adjust:none;
		max-height:1000000px;
		mso-line-height-rule:exactly;
		line-height: 100%
	}
	img {
		display: block;
		border: 0 none !important;
		outline: none;
		text-decoration: none;
	}
	.appleLinks a {color: #000000 !important; text-decoration: none !important;}
	/*Media for Android 4.4.4*/
	/*Media iPhone 6 Plus*/

	@media only screen and (max-width: 640px){
		* {-webkit-text-size-adjust: none}
		p{color:red;}
		/*CSS for Yahoo no spaces*/
		*[class=contenttable]{width:95%!important;}
		.contenttable{width:95%!important;}
		*[class=hide]{display:none!important;}
		.hide{display:none!important;}
		*[class=showmobile]{display:block!important;margin:auto!important;width:100%!important;height:auto!important;max-height:inherit!important;overflow:visible!important;}
		.showmobile{display:block!important;margin:auto!important;width:100%!important;height:auto!important;max-height:inherit!important;overflow:visible!important;}
		*[class=showmobiletable] {display:block!important;margin:auto!important;width:95%!important;height:auto!important;max-height:inherit!important;overflow:visible!important;}
		.showmobiletable{display:block!important;margin:auto!important;width:95%!important;height:auto!important;max-height:inherit!important;overflow:visible!important;}
		*[class=contentInteractive]{display:none!important;}
		.blocktable{width:100%!important;}
		*[class=blocktable]{width:100%!important;}
		.mobile{width:414px!important;height:auto!important;}
		*[class=mobile]{width:414px!important;height:auto!important;}
		*[class=block]{display:block!important;float:none}
		.block{display:block!important;float:none}

	}
	@media yahoo{
		.yahootest {width:200px !important; border:1px solid red !important; display:block !important;}
	}
	*[class=rblock] { margin-top:10px !important; width:320px !important;}

