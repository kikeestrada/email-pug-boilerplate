table.contenttable(width='600', border='0', cellpadding='0', cellspacing='0', align='center')
	tr
		td(valign='top', align='center', width='10', height='60', style='line-height:0; font-size:0px;')
		td(valign='middle', align='center')
			font(style="" +
					"font-family:'Indie Flower', " +
					"sans-serif !important; " +
					"font-family:Arial, " +
					"Helvetica, sans-serif; " +
					"color:#000000;" +
					"font-size:24px; " +
					"line-height:26px"
				)
				| Web Fonts
				// !important must be used after the font-family value to avoid Outlook issue
		td(valign='top', align='center', width='10', height='60', style='line-height:0; font-size:0px;')
